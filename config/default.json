{
  "port": 3000,
  "basePath": "/",
  "docsDir": "./kb",
  "cacheSize": 10000000,
  "trustCloudflare": false,
  "maxDocsLimit": 50,
  "defDocsLimit": 15,
  "useGit": false,
  "searchDelay": 100,
  "date": {
    "options": {
      "month": "short",
      "day": "numeric",
      "year": "numeric"
    },
    "locale": "en-US"
  },
  "jwt": {
    "issuer": "mykb",
    "expiresIn": "7d",
    "audience": "mykb"
  },
  "secret": ""
}
